<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.candao.inorder.dao.InorderCheckTableDao">	<resultMap type="com.candao.inorder.pojo.TblCheck" id="TblCheckResultMap">		<result property="date" column="Date" />		<result property="check" column="Check" />		<result property="barcode" column="BarCode" />		<result property="outlet" column="Outlet" />		<result property="floor" column="Floor" />		<result property="tableno" column="TableNo" />		<result property="oldtableno" column="OldTableNo" />		<result property="cover" column="Cover" />		<result property="checktype" column="CheckType" />		<result property="opentime" column="OpenTime" />		<result property="openperiod" column="OpenPeriod" />		<result property="openstation" column="OpenStation" />		<result property="openstationref" column="OpenStationRef" />		<result property="openemp" column="OpenEmp" />		<result property="closetime" column="CloseTime" />		<result property="closeperiod" column="ClosePeriod" />		<result property="closestation" column="CloseStation" />		<result property="closeemp" column="CloseEmp" />		<result property="printcount" column="PrintCount" />		<result property="lastpage" column="LastPage" />		<result property="lastline" column="LastLine" />		<result property="lastprinttime" column="LastPrintTime" />		<result property="mastertable" column="MasterTable" />		<result property="checktot" column="CheckTot" />		<result property="itemtot" column="ItemTot" />		<result property="sc1" column="SC1" />		<result property="sc2" column="SC2" />		<result property="tax1" column="Tax1" />		<result property="tax2" column="Tax2" />		<result property="tax3" column="Tax3" />		<result property="tax4" column="Tax4" />		<result property="discbefore" column="DiscBefore" />		<result property="discafter" column="DiscAfter" />		<result property="adjustamt" column="AdjustAmt" />		<result property="paytot" column="PayTot" />		<result property="tipstot" column="TipsTot" />		<result property="ispaid" column="IsPaid" />		<result property="taxrate1" column="TaxRate1" />		<result property="taxrate2" column="TaxRate2" />		<result property="taxrate3" column="TaxRate3" />		<result property="taxrate4" column="TaxRate4" />		<result property="scrate1" column="SCRate1" />		<result property="scrate2" column="SCRate2" />		<result property="taxincludemask1" column="TaxIncludeMask1" />		<result property="taxincludemask2" column="TaxIncludeMask2" />		<result property="taxincludemask3" column="TaxIncludeMask3" />		<result property="taxincludemask4" column="TaxIncludeMask4" />		<result property="scincludemask1" column="SCIncludeMask1" />		<result property="scincludemask2" column="SCIncludeMask2" />		<result property="memberid" column="MemberID" />		<result property="islock" column="IsLock" />		<result property="lockemp" column="LockEmp" />		<result property="lockstation" column="LockStation" />		<result property="locktime" column="LockTime" />		<result property="voidMes" column="Void" />		<result property="voidtime" column="VoidTime" />		<result property="voidemp" column="VoidEmp" />		<result property="voidstation" column="VoidStation" />		<result property="voidreason" column="VoidReason" />		<result property="ismodified" column="IsModified" />		<result property="lastmodifiedtime" column="LastModifiedTime" />	</resultMap>	<select id="activeTableInfo" parameterType="map" resultMap="TblCheckResultMap">		SELECT		tc.* FROM tbl_Check AS tc WHERE TableNo=#{tableNo} AND		Date=#{date}		AND		IsPaid="0" AND Floor=#{floor};	</select>	<insert id="insert" parameterType="map">		INSERT INTO `tbl_Check` (		`Date`,`Check`,`BarCode`,`Outlet`,`Floor`,`TableNo`,`Cover`,`CheckType`,		`OpenTime`,`OpenPeriod`,`OpenStation`,`OpenStationRef`,`OpenEmp`,`CheckTot`,`ItemTot`,`IsPaid`,`SCRate1`,`Void`,`IsModified`,`LastModifiedTime`		)		VALUES		(		#{date}, #{check}, #{barcode},#{outlet}, #{floor}, #{tableno},		#{cover}, #{checktype},		#{opentime},#{openperiod},		#{openstation},		#{openstationref},#{openemp}, #{checktot},		#{itemtot},#{ispaid},#{scrate1},#{voidMes},		#{ismodified},		#{lastmodifiedtime}		);	</insert>	<update id="updateCheckTot" parameterType="map">		UPDATE `tbl_Check` SET `CheckTot`=#{checktot}, `ItemTot`=#{itemtot} WHERE		`Date`=#{date} AND `Check`=#{check}	</update></mapper> 