<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.candao.inorder.dao.InorderCheckPeriodDao">	<resultMap type="com.candao.inorder.pojo.TblCheckperiod" id="TblCheckperiodResultMap">		<result property="outlet" column="Outlet" />		<result property="periodno" column="PeriodNo" />		<result property="name1" column="Name1" />		<result property="name2" column="Name2" />		<result property="name3" column="Name3" />		<result property="starttime" column="StartTime" />		<result property="endtime" column="EndTime" />	</resultMap>	<select id="queryCurrentPeriod" resultMap="TblCheckperiodResultMap">	SELECT * FROM tbl_CheckPeriod WHERE StartTime &lt;= CURTIME() ORDER BY StartTime DESC LIMIT 1;	</select></mapper>