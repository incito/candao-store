<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.candao.inorder.dao.InorderItemDao">	<resultMap type="com.candao.inorder.pojo.TblItem" id="TblItemResultMap">		<result property="date" column="Date" />		<result property="check" column="Check" />		<result property="outlet" column="Outlet" />		<result property="itemidx" column="ItemIdx" />		<result property="item" column="Item" />		<result property="name1" column="Name1" />		<result property="name2" column="Name2" />		<result property="name3" column="Name3" />		<result property="names1" column="NameS1" />		<result property="names2" column="NameS2" />		<result property="names3" column="NameS3" />		<result property="parentitemidx" column="ParentItemIdx" />		<result property="childcount" column="ChildCount" />		<result property="type" column="Type" />		<result property="checktype" column="CheckType" />		<result property="moditype" column="ModiType" />		<result property="ordertime" column="OrderTime" />		<result property="orderstation" column="OrderStation" />		<result property="orderemp" column="OrderEmp" />		<result property="shareno" column="ShareNo" />		<result property="seatno" column="SeatNo" />		<result property="fromtable" column="FromTable" />		<result property="printq1" column="PrintQ1" />		<result property="printq2" column="PrintQ2" />		<result property="printq3" column="PrintQ3" />		<result property="printq4" column="PrintQ4" />		<result property="printq5" column="PrintQ5" />		<result property="isprintoncheck" column="IsPrintOnCheck" />		<result property="printstatus" column="PrintStatus" />		<result property="printcheckstatus" column="PrintCheckStatus" />		<result property="printedoncheck" column="PrintedOnCheck" />		<result property="ognprice" column="OgnPrice" />		<result property="price" column="Price" />		<result property="pricelevel" column="PriceLevel" />		<result property="isnonrecal" column="IsNonRecal" />		<result property="modimethod" column="ModiMethod" />		<result property="tax1" column="Tax1" />		<result property="tax2" column="Tax2" />		<result property="tax3" column="Tax3" />		<result property="tax4" column="Tax4" />		<result property="sc1" column="SC1" />		<result property="sc2" column="SC2" />		<result property="discbefore" column="DiscBefore" />		<result property="discafter" column="DiscAfter" />		<result property="moditot" column="ModiTot" />		<result property="unitqty" column="UnitQty" />		<result property="qty" column="Qty" />		<result property="itemtot" column="ItemTot" />		<result property="rvtax1" column="RvTax1" />		<result property="rvtax2" column="RvTax2" />		<result property="rvtax3" column="RvTax3" />		<result property="rvtax4" column="RvTax4" />		<result property="rvsc1" column="RvSC1" />		<result property="rvsc2" column="RvSC2" />		<result property="rvdiscbefore" column="RvDiscBefore" />		<result property="rvdiscafter" column="RvDiscAfter" />		<result property="rvmoditot" column="RvModiTot" />		<result property="rvitemtot" column="RvItemTot" />		<result property="dept" column="Dept" />		<result property="category" column="Category" />		<result property="class" column="Class" />		<result property="disclevel" column="CheckDiscLevel" />		<result property="notshow" column="NotShow" />		<result property="havetax1" column="HaveTax1" />		<result property="havetax2" column="HaveTax2" />		<result property="havetax3" column="HaveTax3" />		<result property="havetax4" column="HaveTax4" />		<result property="havesc1" column="HaveSC1" />		<result property="havesc2" column="HaveSC2" />		<result property="ognhavetax1" column="OgnHaveTax1" />		<result property="ognhavetax2" column="OgnHaveTax2" />		<result property="ognhavetax3" column="OgnHaveTax3" />		<result property="ognhavetax4" column="OgnHaveTax4" />		<result property="ognhavesc1" column="OgnHaveSC1" />		<result property="ognhavesc2" column="OgnHaveSC2" />		<result property="void" column="Void" />		<result property="voidtime" column="VoidTime" />		<result property="voidemp" column="VoidEmp" />		<result property="voidstation" column="VoidStation" />		<result property="voidreason" column="VoidReason" />		<result property="empvoidlevel" column="EmpVoidLevel" />		<result property="totalcost" column="TotalCost" />		<result property="bonus" column="Bonus" />		<result property="isredeemed" column="IsRedeemed" />		<result property="isbonuspaid" column="IsBonusPaid" />		<result property="isitemmodified" column="IsItemModified" />		<result property="lastmodifiedtime" column="LastModifiedTime" />	</resultMap>	<insert id="insertItems" parameterType="java.util.List">		INSERT INTO `tbl_Item` (		`Date`, `Check`, `Outlet`, `ItemIdx`, `Item`,		`Name1`, `Name2`, `Name3`,		`NameS1`, `NameS2`, `NameS3`,		`ParentItemIdx`, `ChildCount`,		`Type`,`OrderTime`, `OrderStation`,		`OrderEmp`,`PrintQ1`, `PrintQ2`, `PrintQ3`, `PrintQ4`,		`PrintQ5`,		`IsPrintOnCheck`,		`PrintStatus`,		`OgnPrice`,		`Price`,		`PriceLevel`,		`ModiMethod`,`UnitQty`,`Qty`,		`ItemTot`, `RvItemTot`,		`Dept`, `Category`,		`Class`,`CheckDiscLevel`,		`TotalCost`,		`IsItemModified`,`LastModifiedTime`) VALUES		<foreach collection="list" item="item" index="index"			separator=",">			(			#{item.date},#{item.check},#{item.outlet},#{item.itemidx},#{item.item},			#{item.name1},#{item.name2},#{item.name3},#{item.names1},#{item.names2},#{item.names3},			#{item.parentitemidx},#{item.childcount},#{item.type},#{item.ordertime},#{item.orderstation},			#{item.orderemp},#{item.printq1},#{item.printq2},#{item.printq3},#{item.printq4},#{item.printq5},			#{item.isprintoncheck},#{item.printstatus},			#{item.ognprice},#{item.price},#{item.pricelevel},#{item.modimethod},			#{item.unitqty},#{item.qty},#{item.itemtot},#{item.rvitemtot},#{item.dept},#{item.category},#{item.classMes},			#{item.disclevel},#{item.totalcost},#{item.isitemmodified},#{item.lastmodifiedtime}			)		</foreach>	</insert>	<select id="queryLastItemInx" parameterType="map"		resultType="java.lang.String">		SELECT  MAX(tbi.ItemIdx) FROM tbl_Item tbi WHERE tbi.`Check`=#{check} AND		tbi.Date=#{date}	</select></mapper>