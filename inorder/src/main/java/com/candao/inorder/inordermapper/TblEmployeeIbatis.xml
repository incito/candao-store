<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- 服务员 --><mapper namespace="com.candao.inorder.dao.InorderEmployeeDao">	<resultMap type="com.candao.inorder.pojo.TblEmployee" id="TblEmployeeResultMap">		<result property="empno" column="EmpNo" />		<result property="emplogin" column="EmpLogin" />		<result property="emppwd" column="EmpPwd" />		<result property="namefirst1" column="NameFirst1" />		<result property="namefirst2" column="NameFirst2" />		<result property="namefirst3" column="NameFirst3" />		<result property="namelast1" column="NameLast1" />		<result property="namelast2" column="NameLast2" />		<result property="namelast3" column="NameLast3" />		<result property="shortname1" column="ShortName1" />		<result property="shortname2" column="ShortName2" />		<result property="shortname3" column="ShortName3" />		<result property="tel" column="Tel" />		<result property="birth" column="Birth" />		<result property="sex" column="Sex" />		<result property="address1" column="Address1" />		<result property="address2" column="Address2" />		<result property="contact" column="Contact" />		<result property="email" column="Email" />		<result property="empdept" column="EmpDept" />		<result property="empauthlevel" column="EmpAuthLevel" />		<result property="emporderlevel" column="EmpOrderLevel" />		<result property="empdisclevel" column="EmpDiscLevel" />		<result property="emppaylevel" column="EmpPayLevel" />		<result property="empvoidlevel" column="EmpVoidLevel" />		<result property="inactive" column="Inactive" />		<result property="empadmin" column="EmpAdmin" />	</resultMap>	<!-- 服务员类型 -->	<resultMap type="com.candao.inorder.pojo.TblEmpdept" id="TblEmpdeptResultMap">		<result property="empdept" column="EmpDept" />		<result property="name1" column="Name1" />		<result property="name2" column="Name2" />		<result property="name3" column="Name3" />	</resultMap>	<!-- 链表查询根据服务员类型查询服务员人数 -->    <resultMap  id="assEmployees" extends="TblEmpdeptResultMap" type="com.candao.inorder.pojo.TblEmpdept" >		<association property="tblEmployees" column="EmpDept" resultMap="TblEmployeeResultMap">		</association>    </resultMap>    <!-- 查询 -->    <select id="queryEmployeeByOpen" parameterType="map" resultMap="assEmployees">    	 SELECT tep.*,tel.* FROM tbl_Empdept tep ,tbl_employee tel    	 <if test="empdept!=null"></if>    	  WHERE  tep.EmpDept=tel.EmpDept;    </select>    <!-- 根据服务员编号查询 服务信息-->    <select id="queryEmployeeForEmpNo" parameterType="map" resultMap="TblEmployeeResultMap">    SELECT * FROM tbl_Employee WHERE EmpNo=#{empNO}    </select></mapper>