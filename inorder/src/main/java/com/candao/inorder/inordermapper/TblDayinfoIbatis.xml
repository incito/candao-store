<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.candao.inorder.dao.InorderDayInfoDao">	<resultMap type="com.candao.inorder.pojo.TblDayinfo" id="TblDayinfo">		<result property="date" column="Date" />		<result property="count" column="Count" />		<result property="starttime" column="StartTime" />		<result property="startemp" column="StartEmp" />		<result property="active" column="Active" />	</resultMap>	<select id="queryActiveDayInfo" resultMap="TblDayinfo">		SELECT * FROM tbl_DayInfo WHERE Active=1;	</select></mapper>