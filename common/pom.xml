				<artifactId>commons-httpclient</artifactId>
				<version>3.1</version>
			</dependency>
 	</dependencies>
 	
 	<build>
  <plugins>
   <plugin>
    <artifactId>maven-compiler-plugin</artifactId>
    <configuration>
     	<source>1.7</source>
     	<target>1.7</target>
     	<encoding>UTF-8</encoding>
    </configuration>
   </plugin>

  </plugins>
 </build>
</project>
