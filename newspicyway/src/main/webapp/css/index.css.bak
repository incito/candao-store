.ky-dropdown{
	position: relative;
}

.ky-dropdown-menu{
	position: absolute;
	top: 0;
	left: 100%;
	z-index: 1000;
	min-width: 160px;
	margin: 0;
	font-size: 14.4px;
	display: none;
	text-align: left;
	list-style: none;
	background-color: #fff;
	-webkit-background-clip: padding-box;
	background-clip: padding-box;
	-webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
	box-shadow: 0 6px 12px rgba(0,0,0,.175);
}

.ky-dropdown-menu .ky-menu-warning{
	background-color: #FFCC4C;
}

.ky-title{
	height: 64px;
	text-align: center;
	padding-top: 12px;
	padding-bottom: 20px;
	background-color: #7AC454;
	padding-right: 20px;
	color: #ffffff;
}
.ky-title>p{
	font-size: 25.6px;
}

.ky-title .navbar-form{
	margin-top: 0;
	margin-bottom: 0;
}

.ky-title .input-group-addon{
	background: #fff;
	border-top-left-radius: 15px;
	border-bottom-left-radius: 15px;
}

.ky-title input[type=text]{
	border-radius: 15px;
	border-left:none;
}

.ky-title-right{
	float: right;
	margin-top: -42px;
}

.ky-title-right > img{
	cursor: pointer;
}

.ky-title-right .form-group{
	position: relative;
	display: table;
}

